#include <iostream>
#include <bits/stdc++.h> 
#include <string>
#include <algorithm>
#include <cmath>
#include <map>
#include <set>
#include <iomanip>
#include <fstream>
#include <sstream>
#include <cstdio>
#include <vector>
#include <cassert>
#include <cstdlib>

using namespace std;


int main() {
	
	string str;
	getline(cin, str);
	
	int str_size = str.length();
	set<char> output;
	
	for(int i=0; i<str_size; i++){
		if(str[i] !='{' && str[i] !='}' && str[i] !=',' && str[i] !=' '){
			output.insert(str[i]);
		}
	}
	cout << output.size() << endl;
 
}
		
