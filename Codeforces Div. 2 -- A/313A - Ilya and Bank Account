#include <iostream>
#include <bits/stdc++.h> 
#include <string>
#include <algorithm>
#include <cmath>
#include <map>
#include <set>
#include <iomanip>
#include <fstream>
#include <sstream>
#include <cstdio>
#include <vector>
#include <cassert>
#include <cstdlib>

using namespace std;


int main() {
		int n, max;
		cin >> n;
		max = n;
		
		if(n/10 > max)
			max = n/10;
		if(n%10 + (n/100)*10 > max)
			max = n%10 + (n/100)*10;

		cout << max << endl;
}
		
