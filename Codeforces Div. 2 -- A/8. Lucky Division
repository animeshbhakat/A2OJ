#include <iostream>
#include <bits/stdc++.h> 
#include <string>
#include <algorithm>
#include <cmath>
#include <map>
#include <set>
#include <iomanip>
#include <fstream>
#include <sstream>
#include <ostream>
#include <cstdio>
#include <vector>
#include <cassert>

using namespace std;

int main()
{
    int testcases;
    cin >> testcases;
    while (testcases--) {
		
		int input;
		cin >> input;
		
		bool flag =0;
		
		int arr[14] = {4, 7, 44, 47, 74, 77,  447, 474, 744, 774, 477, 747, 444, 777};

		for(int i =0; i < 14; i++){
			if(input %arr[i] == 0)
				flag = true;
		}
		
		if(flag)
			cout << "YES" << endl;
		else
			cout << "NO" << endl;
       
    
	}
}