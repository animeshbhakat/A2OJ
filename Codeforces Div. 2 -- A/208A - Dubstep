#include <iostream>
#include <bits/stdc++.h> 
#include <string>
#include <algorithm>
#include <cmath>
#include <map>
#include <set>
#include <iomanip>
#include <fstream>
#include <sstream>
#include <cstdio>
#include <vector>
#include <cassert>
#include <cstdlib>

using namespace std;


	int main(){
		string str;
		int flag =1;
		cin >> str;
		int size = str.size();
		for(int i =0; i<size; i++){
			if(str[i] == 'W' && str[i+1] =='U' && str[i+2] == 'B'){
				i = i+2;
				if(!flag){
					cout << " ";
				}
			}
			else{
				flag = 0;
				cout << str[i];
			}
		}
		cout << " " << endl;
		
}
	
