#include <iostream>
#include <bits/stdc++.h> 
#include <string>
#include <algorithm>
#include <cmath>
#include <map>
#include <set>
#include <iomanip>
#include <fstream>
#include <sstream>
#include <ostream>
#include <cstdio>
#include <vector>
#include <cassert>
#include <cstdlib>

using namespace std;

int main(){
	
		int arr[5][5];
		int row, column;
		for(int i=0; i<5; i++){
			for(int j=0; j<5; j++){
				int p;
				cin >>p;
				
				if(p==0){
				arr[i][j] = p;
				}
				else{
					arr[i][j] = p;
					row = i +1;
					column = j+1;
				}
				
			}
		}
		
		int z = abs(3 - row) + abs(3 - column);
		
		cout << z << endl;

}
		
