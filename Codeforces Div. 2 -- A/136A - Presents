#include <iostream>
#include <bits/stdc++.h> 
#include <string>
#include <algorithm>
#include <cmath>
#include <map>
#include <set>
#include <iomanip>
#include <fstream>
#include <sstream>
#include <ostream>
#include <cstdio>
#include <vector>
#include <cassert>

using namespace std;

int main(){
	
		int input;
		cin >> input;
		int arr1[input+5];
		for(int i=1; i<=input; i++){
			int p;
			cin >>p;
			arr1[p] = i;
		}
		
		for(int j=1; j<=input; j++){
			cout << arr1[j]<< " ";
		}
		
		cout << " " << endl;

}
		
